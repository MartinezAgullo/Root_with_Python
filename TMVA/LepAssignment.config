[input]

filename = lepton_assigment_output.root
filepath = ./
treeName = tHqLoop_nominal_Loose
    
[output]
outfileName = TMVA_output.root

[Features]
variable_target = is_Lep1_from_Top #Label

variables_tmva = pt_lep1:F, pt_lep2:F, pt_lep3:F, eta_lep1:F, eta_lep2:F, eta_lep3:F, phi_lep1:F, phi_lep2:F, phi_lep3:F, mass_lep1:F, mass_lep2:F, mass_lep3:F, charge_lep1:F, charge_lep2:F, charge_lep3:F, deltaR_taulep1:F, deltaR_taulep2:F

variables_spectator = m_njets:I, m_nbjets:I , m_nNoBjets:I, m_nfjets:I, m_nCentraljets:I, weight_nominal:F, runNumber:I, eventNumber:I, m_nleptons:I







[MVA]     
AnalysisType = Classification
#AnalysisType = [Regression, Auto, Classification]

BDT_BoostType = Gradient
#BDT_BoostType =[Gradient, AdaBoostR2] # Regression


Classification_algorithm = Gradient
#Classification_algorithm = [Gradient, LikelihoodD, Fisher, FisherG, BoostedFisher, LD, MLPBNN] # Classification


BDT_depth = 2
BDT_shrink = 0.10
BDT_ntrees = 1000

NegativeWeightTreatment = IgnoreNegWeightsInTraining
#NegativeWeightTreatment = [IgnoreNegWeightsInTraining, Pray, WeightsInverseBoostNegWeights, PairNegWeightsGlobal]


